[alias]
  h = help

  a = add
  aa = add .

  b = branch
  branches = branch -a

  s = status

  f = fetch

  mg = merge

  d = diff
  ds = diff --staged

  cl = clone
  ci = commit
  co = checkout
  com = checkout master
  cod = checkout develop

  stashes = stash list

  l = log
  lo = log --oneline
  lg = log --graph --all  --decorate --oneline
  ll = log --pretty=format:"%C(yellow)%h\\ %ad%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --date=short

  uncommit = reset HEAD~^

  rpo = remote prune origin 
